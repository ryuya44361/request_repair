<main>
  <div class = "container">
    <div class = "row my-3">
      <div class = "col-6">
        <h2>アカウント一覧（会員）</h2>
      </div>
    </div>
    
    
    <div class ="row">
    <div class="col-4 px-5 mt-5">
        <table class="table table-hover table-inverse">
          <thead>
            <tr>
              <th>会員名</th>
              <th>住所</th>
              <th>電話番号</th>
              <th>ステータス</th>
            </tr>
          </thead>

          <tbody>
  
            <% @customers.each do |customer| %>
              <tr>
                <td><%= customer.last_name+customer.first_name %></td>
                <td><%=  address_display %></td>
                <td><%=  %></td>
                <td><%=  %></td>
              </tr>
            <%end%>
          </tbody>
        </table>
        </div>
    </div>
    <div class = "row">
      <div class = "col-10 text-right">
        <%= f.submit '変更', class:"btn btn-success" %>
      </div>
    </div>
  </div>
</main>